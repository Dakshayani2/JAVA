import java.util.*;
class dak {
    public static void main(String[] args) {
        Scanner s = new Scanner(System.in);
        int age;
        System.out.println("enter age:");
        age=s.nextInt();
        if(age>=18){
            System.out.println("eligible for vote");
        }
         else {
        int d=18-age;
        System.out.println("Sorry ! You are Eligible after " + d + " years.\n");
                }
            }
}
        
