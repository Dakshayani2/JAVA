import java.util.*;
public class dak{
    public static void main(String args[]){
        int n,re=0,rem;
        Scanner s=new Scanner(System.in);
        System.out.println("enter a number:");
        n=s.nextInt();
        while(n!=0){
            rem=n%10;
            re=re*10+rem;
            n=n/10;
        }
        System.out.print("the reversed number :"+re);
    }
}
